{
  "restartable": "rs",
  "ignore": [
    ".git",
    ".svn",
    "node_modules/**/node_modules",
    "dist"//避免刷新dist文件后循环更新
  ],
  "verbose": true,
  "execMap": {
    "js": "npm run build && node --harmony"// js文件更新时，执行build
  },
  "watch": [],
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "ejs js json"
}